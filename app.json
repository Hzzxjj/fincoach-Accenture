{
  "expo": {
    "scheme": "fincoach",
    "web": {
      "output": "server",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://your-eas-hosting-domain.com"
        }
      ]
    ],
    "name": "FinCoach",
    "slug": "fincoach",
    "version": "1.0.0",
    "orientation": "portrait",
    // Temporarily disable icon to avoid bad file reference
    // "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        // Temporarily disable adaptive icon image
        // "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      }
    },
    "extra": {
      "router": {
        "origin": "https://your-eas-hosting-domain.com"
      }
    }
  }
}
